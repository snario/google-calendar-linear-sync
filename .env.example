# Google Calendar Linear Sync Configuration
# Copy this file to .env and fill in your values

# Linear API Configuration
LINEAR_API_KEY=lin_api_your_key_here
LINEAR_TEAM_ID=your-team-id-here

# Google Calendar API Configuration  
GOOGLE_SERVICE_ACCOUNT_JSON=base64_encoded_service_account_json_here
GCAL_CALENDAR_ID=your_calendar_id_here

# Optional: History calendar for overdue items
GCAL_HISTORY_CALENDAR_ID=your_history_calendar_id_here